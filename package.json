{
  "name": "@panhezeng/vue-event-hub",
  "description": "A Vue plugin",
  "version": "2.0.1",
  "author": "潘何增 <panhezeng@gmail.com> (http://apsay.com/)",
  "license": "MIT",
  "private": false,
  "keywords": [
    "vuejs",
    "vue plugin",
    "event hub"
  ],
  "homepage": "https://github.com/panhezeng/vue-event-hub#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panhezeng/vue-event-hub.git"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README"
  ],
  "main": "dist/vue-event-hub-index.min.js",
  "scripts": {
    "dev:example": "webpack-dev-server --mode development --open --hot --config webpack.example.config.js",
    "build:example": "rm -rf docs && mkdir docs && cp index.html docs && webpack --mode production --config webpack.example.config.js",
    "build": "webpack --mode production"
  },
  "peerDependencies": {
    "vue": "~2"
  },
  "dependencies": {},
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "vue": "~2",
    "@babel/core": "~7",
    "@babel/plugin-syntax-dynamic-import": "~7",
    "@babel/plugin-transform-runtime": "~7",
    "@babel/preset-env": "~7",
    "@babel/runtime": "~7",
    "@panhezeng/before-run-webpack-plugin": "~1",
    "babel-loader": "~8",
    "css-loader": "~2",
    "vue-loader": "~15",
    "vue-template-compiler": "~2",
    "webpack": "~4",
    "webpack-cli": "~3",
    "webpack-dev-server": "^3.1.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,css,md,jsx,vue,ts,less}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/**",
      "**/docs/**"
    ]
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
